rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    
    // WARNING: These rules are insecure and are meant for development purposes only.
    // They allow any authenticated user to read and write to any document.
    // You MUST secure these rules before deploying to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
